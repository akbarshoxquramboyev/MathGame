<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.math.mathforeveryone">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MathForEveryone">
        <activity
            android:name="com.math.mathforeveryone.KenmaKetlik"
            android:configChanges="orientation"
            android:screenOrientation="landscape" />
        <activity
            android:name="com.math.mathforeveryone.FindMissingNumber"
            android:configChanges="orientation"
            android:screenOrientation="landscape" />
        <activity
            android:name="com.math.mathforeveryone.GameSequence"
            android:configChanges="orientation"
            android:screenOrientation="landscape" />
        <activity
            android:name="com.math.mathforeveryone.GameSubtract"
            android:configChanges="orientation"
            android:screenOrientation="landscape" />
        <activity
            android:name="com.math.mathforeveryone.RightAnswer"
            android:configChanges="orientation"
            android:screenOrientation="landscape" />
        <activity
            android:name="com.math.mathforeveryone.GameAdd"
            android:configChanges="orientation"
            android:screenOrientation="landscape" />
        <activity
            android:name="com.math.mathforeveryone.ChooseCategory"
            android:configChanges="orientation"
            android:screenOrientation="landscape" />
        <activity
            android:name="com.math.mathforeveryone.MainActivity"
            android:configChanges="orientation"
            android:exported="true"
            android:screenOrientation="landscape">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>